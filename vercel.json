{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "rewrites": [],
    "redirects": [],
    "headers": [],
    "env": {
      "REACT_APP_API_URL": "https://form-builder-app-backend.vercel.app"
    }
  }
  